@import "../../component/message/message.wxss";
page {
  background: #eff2f5;
}

.user-info {
  box-sizing: border-box;
  width: 750rpx;
  height: 300rpx;
  padding: 30rpx 0;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-around;
  background-image: linear-gradient(45deg, #234567, #cdd01f);
  background-position: center;
  background-repeat: no-repeat;
  background-size: cover;
  color: #666;
}

.user-nickname {
  color: #fff;
  font-size: 30rpx;
  text-shadow: 1px 1px 0 rgba(0, 0, 0, .6);
}

.user-sex-male,
.user-sex-female {
  display: inline-block;
  width: 40rpx;
  height: 40rpx;
  margin-bottom: -5rpx;
  background-position: center;
  background-repeat: no-repeat;
  background-size: cover;
}

.user-sex-male {
  background-image: url(data:image/png;base64,);
}

.user-sex-female {
  background-image: url(data:image/png;base64,);
}

.user-avatar {
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
  box-shadow: 0 0 300rpx 100rpx rgba(255, 255, 255, .2), 0 1px 0 rgba(255, 255, 255, 0.4);
}

.user-location {
  font-size: 24rpx;
  color: #eee;
  text-shadow: 1px 1px 0 rgba(0, 0, 0, .6);
}

.grids {
  background: #fff;
  margin-top: 30rpx;
  padding: 0 20rpx;
  border-top: 1px solid #efefef;
  border-bottom: 1px solid #efefef;
}

.grid {
  position: relative;
  background-repeat: no-repeat;
  background-position: 680rpx center;
  background-size: 30rpx 30rpx;
  padding: 5rpx 0;
  line-height: 70rpx;
  border-bottom: 1px solid #efefef;
  background-image: url('data:image/png;base64,');
}

.grid:last-child {
  border-bottom: none;
}

.grid-icon {
  position: absolute;
  left: 0rpx;
  top: 10rpx;
  width: 50rpx;
  height: 50rpx;
  background-position: center;
  background-repeat: no-repeat;
  background-size: contain;
}

.grid-icon.info {
  background-image: url('data:image/png;base64,');
}

.grid-icon.set {
  background-image: url('data:image/png;base64,');
}

.grid-icon.recommend {
  background-image: url('data:image/png;base64,');
}

.grid-icon.protocol {
  background-image: url('data:image/png;base64,');
}

.grid-icon.cancel {
  background-image: url('data:image/png;base64,');
}

.grid button {
  position:absolute;
  width:100%;
  height:100%;
  top:0;
  left:0;
  opacity:0;
  z-index: 2;
}

.grid-title {
  display: inline-block;
  font-size: 28rpx;
  padding-left: 80rpx;
}